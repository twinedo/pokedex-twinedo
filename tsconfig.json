{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"*": ["src/*"],
			"tests": ["tests/*"],
			"@assets/*": ["assets/*"],
			"@components/*": ["src/components/*"],
			"@navigations/*": ["src/navigations/*"],
			"@screens/*": ["src/screens/*"],
			"@services/*": ["src/services/*"],
			"@stores/*": ["src/stores/*"],
			"@styles/*": ["src/styles/*"],
			"@utils/*": ["src/utils/*"]
		}
	}
}
